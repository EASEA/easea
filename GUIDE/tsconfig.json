{
  "typedocOptions": {
    "entryPoints": ["src/index.ts"],
    "out": "documentation/code",
    "exclude": "src/*.py"
  },
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": false,
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
        "src/*":[
            "./src/*"
        ],
    }
  },
  "include": ["**/*"]
}
